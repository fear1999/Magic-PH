    },
    {
      "type": "pwa-chrome",
      "request": "attach",
      "name": "Attach to browser",
      "port": 9222
    },
    {
      "name": "SC",
      "type": "pwa-chrome",
      "request": "launch",
      "runtimeExecutable": "D:\\Docs\\Github\\Magic-PH\\chrome\\bin\\chrome.exe",
      "userDataDir": "${workspaceFolder}/chrome/profile",
      "url": "https://pornhub.com/"
    }
  ]
}
