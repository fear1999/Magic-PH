{
  "version": "0.2.0",
  "compounds": [
      {
        "name": "Attach/Launch",
        "configurations": ["SF","Attach:Firefox"]
      }
    ],
  "configurations": [
      {
        "name": "Attach:Firefox",
        "type": "firefox",
        "request": "attach",
      },
