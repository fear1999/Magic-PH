{
  "name": "magicph",
  "productName": "MagicPH",
  "version": "1.0.52",
  "description": "Enhances your porn viewing experience. Quick, clean, and easy.",
  "scripts": {
    "dev:Start": "concurrently \"npm run dev:Firefox\" \"npm run dev:Sass\"",
    "dev:Firefox": "webpack --progress --mode development --config=webpackConfig/webpack.config.js",
    "dev:Chrome": "webpack --progress --mode development --config=webpackConfig/webpack_ch.config.js",
    "dev:Sass": "sass -w ./src/sass/magicph.scss:./dist/css/magicph.css ./src/sass/downloader.scss:./dist/css/downloader.css ./src/sass/plyr/plyr.scss:./dist/css/plyr.css -s compressed",
    "analyse-bundle": "ANALYSE= webpack --mode production --config=webpackConfig/webpack.config.js",
    "build:Firefox": "webpack --mode production --config=webpackConfig/webpack.config.js",
    "build:Chrome": "webpack --mode production --config=webpackConfig/webpack_ch.config.js",
    "build:Crx": "node webpackConfig/buildChrome.js",
    "build:Browsers": "npm run build:Sass && npm run build:Firefox && npm run build:Chrome && npm run build:Crx"
  },
  "author": "Magic <magicoflolis@tuta.io>",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/magicoflolis/Magic-PH/issues"
  },
  "homepage": "https://github.com/magicoflolis/Magic-PH",
  "devDependencies": {
    "@ffmpeg/core": "^0.10.0",
    "@ffmpeg/ffmpeg": "^0.10.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.189",
    "@types/chrome": "^0.0.188",
    "@types/dotenv-webpack": "^7.0.3",
    "@types/firefox-webext-browser": "latest",
    "@types/jquery": "^3.5.14",
    "@types/path-browserify": "^1.0.0",
    "@types/sass": "^1.43.1",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "autoprefixer": "^10.4.7",
    "copy-webpack-plugin": "^11.0.0",
    "dotenv": "^16.0.1",
    "dotenv-webpack": "^7.1.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "html-loader": "^3.1.0",
    "path-browserify": "^1.0.1",
    "swc-loader": "^0.2.1",
    "terser-webpack-plugin": "^5.3.1",
    "web-ext": "^6.8.0",
    "webpack": "^5.72.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "browserslist": "^4.20.3",
    "crx": "^5.0.1",
    "globby": "^13.1.1",
    "jquery": "^3.6.0",
    "sass": "^1.52.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magicoflolis/Magic-PH.git"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true,
      "jquery": true,
      "node": false,
      "webextensions": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": false,
      "ecmaFeatures": {
        "globalReturn": true,
        "arrowFunctions": true,
        "modules": true
      },
      "ecmaVersion": "latest"
    },
    "rules": {}
  },
  "keywords": [
    "magicph",
    "ph",
    "pornhub",
    "pornhubpremium"
  ],
  "private": true
}
