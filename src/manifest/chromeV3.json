{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "description": "__MSG_extDesc__",
  "default_locale": "en",
  "short_name": "MagPH",
  "background": {
    "service_worker": "/js/service-worker.js",
    "type": "module"
  },
  "declarative_net_request": {
    "rule_resources": [
    ]
  },
  "action": {
    "default_icon": {
      "64": "img/icon_64.png",
      "128": "img/icon_128.png"
    },
    "default_title": "MagicPH - Popup",
    "default_popup": "options.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "all_frames": true,
      "css": [
        "/css/magicph.css",
        "/css/downloader.css"
      ],
      "js": [
        "/js/mph.js",
        "/js/mph-client.js",
        "/js/mph-common.js",
        "/js/mph-adremover.js",
        "/js/start.js",
        "/js/custom-player.js"
      ],
      "matches": [
        "https://*.pornhub.com/*",
        "https://*.pornhub.es/*",
			  "https://*.pornhubpremium.com/*",
			  "https://*.youporn.com/*",
			  "https://*.youpornpremium.com/*",
			  "https://*.youporngay.com/*",
			  "https://*.redtube.com/*",
			  "https://*.redtubepremium.com/*",
			  "https://*.tube8.com/*",
			  "https://*.thumbzilla.com/*",
        "https://onlyfans.com/*",
        "https://xhamster.com/*",
        "https://*.xvideos.com/*"
      ],
      "exclude_matches": [
        "https://*.pornhub.com/login",
        "https://*.pornhub.com/create_account_select",
        "https://*.pornhubpremium.com/login",
        "https://*.pornhubpremium.com/create_account_select",
        "https://*.pornhubpremium.com/gift"
      ],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "64": "img/icon_64.png",
    "128": "img/icon_128.png"
  },
  "permissions": [
    "cookies",
		"webNavigation",
    "declarativeNetRequest",
    "activeTab",
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "*://*.trafficjunky.com/*",
    "*://*.hotjar.com/*",
    "*://*.phncdn.com/*",
    "*://*.pornhub.com/*",
    "*://*.pornhub.es/*",
		"*://*.pornhubpremium.com/*",
		"*://*.youporn.com/*",
		"*://*.youpornpremium.com/*",
		"*://*.youporngay.com/*",
		"*://*.redtube.com/*",
		"*://*.redtubepremium.com/*",
		"*://*.tube8.com/*",
		"*://*.thumbzilla.com/*",
    "*://onlyfans.com/*",
    "*://*.ahcdn.com/*",
    "*://*.xhcdn.com/*",
    "*://xhamster.com/*",
    "*://xhamster.desi/*",
    "*://xhamster.one/*",
    "*://*.xvideos.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [ "/web_accessible_resources/*", "js/*", "css/plyr.css", "img/*" ],
      "matches": [
        "https://*.pornhub.com/*",
        "https://*.pornhub.es/*",
			  "https://*.pornhubpremium.com/*",
			  "https://*.youporn.com/*",
			  "https://*.youpornpremium.com/*",
			  "https://*.youporngay.com/*",
			  "https://*.redtube.com/*",
			  "https://*.redtubepremium.com/*",
			  "https://*.tube8.com/*",
			  "https://*.thumbzilla.com/*",
        "https://onlyfans.com/*",
        "https://xhamster.com/*",
        "https://*.xvideos.com/*"
      ]
    }
  ],
  "content_security_policy.extension_pages": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_security_policy.sandbox": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "key": "",
  "update_url": "https://raw.githubusercontent.com/magicoflolis/Magic-PH/master/version/updates.xml"
}
